<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/groupoptions.xsd">
    <import>
        <categories>
            <category name="user.board.serverlist">
                <parent>user.board</parent>
                <showorder>20</showorder>
            </category>
            <category name="admin.community.serverlist">
                <parent>admin.community</parent>
                <showorder>20</showorder>
            </category>
        </categories>
        <options>
            <option name="user.board.serverlist.canView">
                <categoryname>user.board.serverlist</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
                <showorder>1</showorder>
                <visible>1</visible>
            </option>
            <option name="admin.community.serverlist.canView">
                <categoryname>admin.community.serverlist</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>1</showorder>
                <enableoptions>admin.community.serverlist.canAdd,admin.community.serverlist.canEdit,admin.community.serverlist.canDelete</enableoptions>
                <visible>1</visible>
            </option>
            <option name="admin.community.serverlist.canAdd">
                <categoryname>admin.community.serverlist</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>2</showorder>
                <visible>1</visible>
            </option>
            <option name="admin.community.serverlist.canEdit">
                <categoryname>admin.community.serverlist</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>3</showorder>
                <visible>1</visible>
            </option>
            <option name="admin.community.serverlist.canDelete">
                <categoryname>admin.community.serverlist</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>4</showorder>
                <visible>1</visible>
            </option>
        </options>
    </import>
</data>